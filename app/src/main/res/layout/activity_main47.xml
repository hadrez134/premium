<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="apps.bienestar.pausasactivasybienestar.Main47Activity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView438"
                android:layout_width="match_parent"
                android:layout_height="57dp"
                android:fontFamily="@font/andada"
                android:text="@string/app_pag541"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="16sp"
                android:textStyle="bold"
                android:typeface="serif"
                tools:text="@string/app_pag541" />

            <TextView
                android:id="@+id/textView439"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/andada"
                android:text="@string/app_pag542"
                android:textColor="#000000"
                android:textSize="16sp"
                android:typeface="serif"
                tools:text="@string/app_pag542" />

            <view
                android:id="@+id/prin"
                class="com.google.android.youtube.player.YouTubePlayerView"
                android:layout_width="match_parent"
                android:layout_height="319dp" />

            <Button
                android:id="@+id/boton"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:fontFamily="@font/andada"
                android:text="@string/generales"
                android:textSize="16sp"
                android:textStyle="italic"
                android:typeface="serif"
                tools:text="@string/generales" />

            <TextView
                android:id="@+id/textView440"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/andada"
                android:text="@string/app_533"
                android:textColor="#000000"
                android:textSize="16sp"
                tools:text="@string/app_533" />

            <Button
                android:id="@+id/butt2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/andada"
                android:onClick="normal"
                android:text="@string/app_314"
                android:textSize="16sp"
                android:textStyle="italic"
                android:typeface="serif"
                tools:text="@string/app_314" />
        </LinearLayout>
    </ScrollView>
</FrameLayout>